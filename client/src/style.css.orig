p.involved-para.MT-first-sec {
    text-align: center;
    padding: 0 40px;
    font-size: 1.4em;
    margin-bottom: 60px;
}

.volunteer-img{
    width: 100%;
    margin:0 auto;
    
    object-fit: cover;
    object-position: 50% 62%;
    height: 350px;
    
}

