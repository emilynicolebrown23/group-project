/* Footer ///////////////////////////////////////// */

.last-section p, .last-section li,.last-section h3, .last-section h2{
    color: #f7f7f7;
    font-weight: 100;

}

.last-section h3, .last-section h2{
    color: #D49591;
}

.pplgrid{
    background-image: url(./assets/pplgrid.jpg);
    background-repeat:no-repeat; 
    background-size: cover;
    border: none;
    width: 100%;
    /* height: 130px; */
    border-bottom: 2px solid #000;
}

.copyrightInfo li{
    
    padding:25px 25px;
    /* margin:0 50px; */
}

.copyrightInfo{
    width: 100%;
    display: flex;
    justify-content: center;
    justify-items: center;
    margin-bottom: 0;
    
    
}
.rborder{
    border-right: 2px solid rgb(86, 84, 83);
    margin-right:1em;
    padding: 0 !important;
}
span.sec-num {
    margin-left: 20px;
}
.last-section ul {
    padding: 0;
}  

.last-section ul li {
    display: inline-block;
    margin-right: 20px;
}

.footer .nav, .footer .copyrightInfo{
    background: #252424;
}

.footer li.nav-item a {
    font-size: 1em;
    font-family: Open Sans, sans-serif;
    transition: .3s;
}
.footer li.nav-item a:hover {
    color: #D49591;
}

aside.ScrollUpButton__Container.ScrollUpButton__Toggled {
    border: 3px solid #D49591 !important;
}
img.logo-main-footer {
    position: relative;
    bottom: 7px;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    
}
.last-section {
    background: #47444B;
    padding: 30px 0;
}


