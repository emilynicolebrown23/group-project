/* Resources Page Styles ///////////////////////////////////////// */
.resources-img {
    /* height: 350px; */
    width: 100%;
    margin: 0 auto;
    object-fit: cover;
    object-position: 50% 62%;
    height: 350px;
    margin-bottom: 60px;
}

.resources-img-2{
    /* height: 330px; */
    width:100%;
    margin-top:70px;
  
}


.resources-container {
    background-color: lightpink;
    height: 100vh;
}


