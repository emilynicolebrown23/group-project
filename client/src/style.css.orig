@import url('https://fonts.googleapis.com/css?family=PT+Serif|Rufina:700|Playfair+Display:400,700');
@import url('https://fonts.googleapis.com/css?family=Amatic+SC:700');

* {
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
    /* font-size: 30px; */
    
}
body {
    background-color: rgb(243, 245, 247);
    
}

div{
color:#5f5061;
}

ul {
    list-style: none;
}

.MT-first-sec{
    margin-top: 60px;
}

.MT-second-sec{
    margin-top: 70px;
}


/* Home page style------------------------------- */
.carousel-caption.d-none.d-md-block p {
    display: none;
}

.carousel-caption.d-none.d-md-block {
    /* font-size: 3em; */
    /* background: #ffffff63;
    border-radius: 5px; */
}

.carousel-caption.d-none.d-md-block h3 {
    font-size: 5em;
    border-radius: 5px;
    color: #fff;
    font-family: 'Amatic SC', cursive;
    font-weight: 700;
}
.date h2 {
    color: #D49591;
    padding-right: 10px;
    font-size: 3em;
    text-align: center;
    margin: 0;
    padding: 0;
}
.date p{
    color: #5F5061;
    padding-right: 10px;
    font-size: 2em;
    text-align: center;
    margin: 0;
    padding: 0;
}

.media-body {
    border-left: 2px solid #D49591;
    padding-left: 15px;
    margin-left: 25px;
    margin-bottom: 40px;
}
.media-heading {
    color: #f7f7f7;
    background: linear-gradient(to left, rgba(212, 149, 145, 0), #5F5061);
    padding: 6px;
    border-radius: 3px;
    font-weight: 100;
    position: relative;
    width: 90%;
    padding-right: 90px;
}
.event-sec{
    background: #ffffff;
    padding: 30px;
    margin-bottom: 50px;
}

.big-link-prevent{
    background-image: url(./assets/big-links_0000.jpg);
    background-repeat:no-repeat; 
    background-size: cover;
    border: none;
    width: 360px;
    height: 230px;
}
.big-link-involved{
    background-image: url(./assets/big-link_0001.jpg);
    background-repeat:no-repeat; 
    background-size: cover;
    border: none;
    width: 360px;
    height: 230px;
}
.big-link-resources{
    background-image: url(./assets/big-link_0002.jpg);
    background-repeat:no-repeat; 
    background-size: cover;
    border: none;
    width: 360px;
    height: 230px;
}

.big-link-prevent a, .big-link-resources a, .big-link-involved a {
    position: absolute;
    /* margin-top:30px; */
    background: rgba(71, 68, 75, 0.7);
    border: none;
    width: 100%;
    border-radius: 0;
    bottom: -1px;
    font-size: 1.5em;   
    font-family: 'Amatic SC', cursive;
    /* font-weight: 700; */
}


.big-link-resources.card-body, .big-link-prevent.card-body, .big-link-involved.card-body {
    padding: 0;
}


.card {
    border:none;
}


.card-title, p.card-text  {
    color: #fff;
    text-align: center;
}

 form.search-form {
    border-radius: 2px !important;
    padding: 6px 20px;
    position: relative;
    margin-left: 20px;
}
