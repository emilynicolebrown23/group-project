}

h5.list-group-item-heading {
    color: #ffffff;
    background: linear-gradient(to left, rgba(212, 149, 145, 0), #5F5061);
    padding: 6px;
    border-radius: 3px;
    font-weight: 100;
    position: relative;
    width: 90%;
}

.involved-container form h2 {
    font-size: 2.3em;
    letter-spacing: 0;
    font-weight: 100;
